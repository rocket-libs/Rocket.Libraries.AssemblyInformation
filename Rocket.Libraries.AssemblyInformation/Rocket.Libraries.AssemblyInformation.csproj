<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Version>1.0.0</Version>
    <Authors>Rocket Libraries</Authors>
    <Company>Rocket Libraries</Company>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <Description>A simple library to return basic information about the current executing assembly. Also provides a basic MVC controller endpoint to allow reading of assembly version over HTTP</Description>
    <PackageProjectUrl>https://github.com/rocket-libs/Rocket.Libraries.AssemblyInformation</PackageProjectUrl>
    <RepositoryUrl>https://github.com/rocket-libs/Rocket.Libraries.AssemblyInformation</RepositoryUrl>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore.Mvc.Core" Version="2.2.5" />
  </ItemGroup>

</Project>
